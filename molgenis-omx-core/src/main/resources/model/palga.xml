<?xml version="1.0" encoding="UTF-8"?>
<molgenis name="org.molgenis">
	<module name="palga">
		<entity name="PalgaSample" label="PALGA">
			<field name="id" type="autoid" hidden="true" />
			<field name="diagnose" label="Diagnose" type="mref" xref_entity="Diagnosis" aggregateable="true" />
			<field name="materiaal" label="Materiaal" type="xref" xref_entity="Material" aggregateable="true" />
			<field name="jaar" label="Jaar" description="Jaar van verslaglegging" type="int" min="1980" max="2014" aggregateable="true" />
			<field name="geslacht" label="Geslacht" type="xref" xref_entity="Gender" aggregateable="true" />
			<field name="leeftijd" label="Leeftijd" type="int" aggregateable="true" />
		</entity>
		<entity name="Diagnosis" label="Diagnose" xref_label="identifier" xref_lookup="identifier,description">
			<field name="id" type="autoid" hidden="true" />
			<field name="identifier" label="Code" type="string" />
			<field name="description" label="Omschrijving" type="string" />
			<unique fields="identifier" />
		</entity>
		<entity name="Material" label="Materiaal" xref_label="type" xref_lookup="type">
			<field name="id" type="autoid" hidden="true" />
			<field name="type" type="string" />
			<unique fields="type" />
		</entity>
		<entity name="Gender" label="Geslacht" xref_label="gender" xref_lookup="gender">
			<field name="id" type="autoid" hidden="true" />
			<field name="gender" type="string" />
			<unique fields="gender" />
		</entity>
	</module>
</molgenis>